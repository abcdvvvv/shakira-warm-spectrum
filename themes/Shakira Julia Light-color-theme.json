{
    "name": "Shakira Julia Light",
    "colors": {
        "titleBar.activeBackground": "#ffffff",
        "titleBar.activeForeground": "#333333",
        "titleBar.inactiveBackground": "#f2f2f2",
        "titleBar.inactiveForeground": "#33333399",
        "activityBar.background": "#ffffff",
        "activityBar.foreground": "#333333",
        "activityBar.inactiveForeground": "#33333399",
        "activityBar.activeBackground": "#f2f2f2",
        "activityBar.activeBorder": "#CB3C33",
        "statusBar.background": "#f2f2f2",
        "statusBar.noFolderBackground": "#f2f2f2",
        "statusBar.foreground": "#333333",
        "statusBar.debuggingBackground": "#4063D8",
        "editorBracketHighlight.foreground1": "#4063D8",
        "editorBracketHighlight.foreground2": "#9558B2",
        "editorBracketHighlight.foreground3": "#CB3C33",
        "editorBracketHighlight.foreground4": "#e77b00",
        "editorBracketHighlight.foreground5": "#389826"
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "string": "#476ae0",
        "variable": "#333333",
        "keyword": "#CB3C33",
        "struct": "#CB3C33",
        "number": "#e77b00",
        "operator": "#333334",
        "function": "#4063D8",
        "method": "#4063D8",
        "macro": "#4063D8",
        "type": "#389826",
        "class": "#389826",
        "property": "#a9b7c6"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [ "comment",
                "punctuation.definition.comment" ],
            "settings": { "fontStyle": "italic", "foreground": "#546E7A" }
        },
        {
            "name": "Variables",
            "scope": [ "variable",
                "string constant.other.placeholder" ],
            "settings": { "foreground": "#333333" }
        },
        {
            "name": "Colors",
            "scope": [ "constant.other.color" ],
            "settings": { "foreground": "#ffffff" }
        },
        {
            "name": "Invalid",
            "scope": [ "invalid",
                "invalid.illegal" ],
            "settings": { "foreground": "#FF5370" }
        },
        {
            "name": "Keyword, Storage",
            "scope": [ "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier" ],
            "settings": { "foreground": "#CB3C33" }
        },
        {
            "name": "Operator, Misc",
            "scope": [ "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution" ],
            "settings": { "foreground": "#333334" }
        },
        {
            "name": "Punctuation (commas, terminators)",
            "scope": [ "punctuation.separator",
                "punctuation.separator.comma",
                "punctuation.separator.parameter",
                "punctuation.separator.delimiter",
                "punctuation.terminator.statement" ],
            "settings": { "foreground": "#333333" }
        },
        {
            "name": "Punctuation (string quotes)",
            "scope": [ "punctuation.definition.string",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end" ],
            "settings": { "foreground": "#476ae0" }
        },
        {
            "name": "Tag",
            "scope": [ "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter" ],
            "settings": { "foreground": "#CB3C33" }
        },
        {
            "name": "Function, Special Method",
